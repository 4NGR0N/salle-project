; BIND reverse data file for empty rfc1918 zone
;
; DO NOT EDIT THIS FILE - it is used for multiple zones.
; Instead, copy it, edit named.conf, and use that copy.
;
$TTL	86400
@	IN	SOA	dns-node.controlwizard.local. root.controlwizard.local. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
;
@	IN	NS	dns-node.controlwizard.local.

; Registros de los servidores de dominio

66	IN	PTR dns-node.controlwizard.local.

; Registros inversos de las maquinas del dominio

130	IN	PTR	tower-node.controlwizard.local.
65	IN	PTR	nagios-node.controlwizard.local.
67	IN	PTR	dhcp-node.controlwizard.local.
69	IN	PTR	ftp-node.controlwizard.local.
131	IN	PTR	bbdd-node.controlwizard.local.
194	IN	PTR	web-node.controlwizard.local.


